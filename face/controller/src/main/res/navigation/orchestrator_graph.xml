<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/orchestrator_graph"
    app:startDestination="@id/blankFragment">

    <action
        android:id="@+id/action_global_errorFragment"
        app:destination="@id/graph_alert"
        app:popUpTo="@id/orchestrator_graph"
        app:popUpToInclusive="true" />

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.simprints.face.orchestrator.BlankFragment"
        android:label="BlankFragment" />

    <include app:graph="@navigation/graph_alert" />

</navigation>
